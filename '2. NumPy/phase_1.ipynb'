    {
      "cell_type": "markdown",
      "source": [
        "## Python List vs NumPy Array"
      ],
      "metadata": {
        "id": "To_OVPynjLCa"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "py_list = [1, 2, 3]\n",
        "print(\"Python list multiplication: \\n\", py_list * 2)\n",
        "\n",
        "\n",
        "np_array = np.array([1, 2, 3]) #element wise multiplication\n",
        "print(\"Python array multiplication: \\n\", np_array * 2)"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
        "id": "Sj1q8cnhjLtv",
        "outputId": "1e4313d1-95ad-4d3d-bcb4-d5ff2281afc8"
      },
      "execution_count": null,
      "outputs": [
        {
          "output_type": "stream",
          "name": "stdout",
          "text": [
            "Python list multiplication: \n",
            " [1, 2, 3, 1, 2, 3]\n",
            "Python array multiplication: \n",
            " [2 4 6]\n"
          ]
        }
      ]
    },
    {
      "cell_type": "code",
      "source": [
        "import time\n",
        "\n",
        "tick = time.time()\n",
        "py_list = [i*2 for i in range(6000000)]\n",
        "tock = time.time()\n",
        "py_list_time = tock - tick\n",
        "print(\"\\nList operation time: \", py_list_time)\n",
        "\n",
        "tick = time.time()\n",
        "np_array = np.arange(6000000) * 2\n",
        "tock = time.time()\n",
        "np_array_time = tock - tick\n",
        "print(\"\\nNumpy operation time: \", np_array_time)\n",
        "\n",
        "if py_list_time > np_array_time:\n",
        "  print(f\"\\nNumpy is {py_list_time/np_array_time} times faster than Python\")\n",
        "else:\n",
        "  print(f\"\\nPython is {np_array_time/py_list_time} times then Numpy\")"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
        "id": "7NTA-KEPjgNt",
        "outputId": "58e1b949-b8cc-427a-cac5-4765db8171f3"
      },
      "execution_count": null,
      "outputs": [
        {
          "output_type": "stream",
          "name": "stdout",
          "text": [
            "\n",
            "List operation time:  0.46894192695617676\n",
            "\n",
            "Numpy operation time:  0.019382476806640625\n",
            "\n",
            "Numpy is 24.194117791773273 times faster than Python\n"
          ]
        }
      ]
    },
    {
      "cell_type": "code",
      "source": [
        "import time\n",
        "\n",
        "py_list = [i for i in range(6000000)]\n",
        "\n",
        "np_array = np.arange(6000000)\n",
        "\n",
        "tick = time.time()\n",
        "for i in range(6000000):\n",
        "  py_list[i] = (py_list[i] + py_list[i]) * 2\n",
        "tock = time.time()\n",
        "\n",
        "py_list_time = tock - tick\n",
        "print(\"\\nList operation time: \", py_list_time)\n",
        "\n",
        "tick = time.time()\n",
        "np_array = (np_array + np_array) * 2\n",
        "tock = time.time()\n",
        "\n",
        "py_list_time = tock - tick\n",
        "print(\"\\nNumpy operation time: \", py_list_time)"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
        "id": "e34-wZH-QNiz",
        "outputId": "ce9f86a7-7c7f-4f37-8bb3-c416dc13069c"
      },
      "execution_count": 8,
      "outputs": [
        {
          "output_type": "stream",
          "name": "stdout",
          "text": [
            "\n",
            "List operation time:  1.0904953479766846\n",
            "\n",
            "Numpy operation time:  0.02868962287902832\n"
          ]
        }
      ]
    },
