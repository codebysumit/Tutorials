        "# Staking"
      ],
      "metadata": {
        "id": "j3HN4e5Kmb81"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "a = np.random.randint(0, 100, size=(2, 2), dtype=np.int8)\n",
        "b = np.random.randint(0, 100, size=(2, 2), dtype=np.int8)\n",
        "\n",
        "print(a, \"\\n\")\n",
        "print(b)"
      ],
