      "cell_type": "code",
      "source": [
        "# y = np.random.random((4, 4))\n",
        "y = np.random.randint(0, 100, size=(4, 4), dtype=np.int8)\n",
        "\n",
        "\n",
        "print(\"Original array: \\n\", y)\n",
        "print(f\"Shape: {y.shape}\")\n",
        "print()\n",
        "\n",
        "y = np.expand_dims(y, axis=0)\n",
        "print(\"Expanded array: \\n\", y)\n",
        "print(f\"Shape: {y.shape}\")\n",
        "print()\n",
        "\n",
        "y = np.squeeze(y)\n",
        "print(\"Squeezed array: \\n\", y)\n",
        "print(f\"Shape: {y.shape}\")"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
