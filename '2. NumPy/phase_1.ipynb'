      "execution_count": null,
      "outputs": [
        {
          "output_type": "stream",
          "name": "stdout",
          "text": [
            "Original array: \n",
            " [[55 63 78 70]\n",
            " [79 90 56  0]\n",
            " [97 60 69 51]\n",
            " [18  1 38  6]]\n",
            "Shape: (4, 4)\n",
            "\n",
            "Expanded array: \n",
            " [[[55 63 78 70]\n",
            "  [79 90 56  0]\n",
            "  [97 60 69 51]\n",
            "  [18  1 38  6]]]\n",
            "Shape: (1, 4, 4)\n",
            "\n",
            "Squeezed array: \n",
            " [[55 63 78 70]\n",
            " [79 90 56  0]\n",
            " [97 60 69 51]\n",
            " [18  1 38  6]]\n",
            "Shape: (4, 4)\n"
          ]
        }
      ]
